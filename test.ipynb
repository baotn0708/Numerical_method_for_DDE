    "class DenseSegment:\n",
    "    def __init__(self, t_n, y_n, k_list, h):\n",
    "        self.t0 = t_n\n",
    "        self.y0 = y_n\n",
    "        self.k  = k_list  # list of 6 mảng derivative tại các stage\n",
